<?xml version='1.0'?>

<robot name="robot" xmlns:xacro="http://www.ros.org/wiki/xacro">

<xacro:include filename="$(find DINGO)/urdf/DINGO.gazebo" />


  <link name="robot_footprint"></link>

  <joint name="robot_footprint_joint" type="fixed">
    <origin xyz="0 0 0" rpy="0 0 0" />
    <parent link="robot_footprint"/>
    <child link="chassis" />
  </joint>

  <link name='chassis'>
    <pose>0 0 0 0 0 0</pose>

    <inertial>
      <mass value="90.0"/>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="${90*1/12*.876*.876}" ixy="0" ixz="0"
          iyy="${90*1/12*.787*.787}" iyz="0"
          izz="${90*1/12*(.787*.787+.876*.876)}"
      />
    </inertial>

    <collision name='collision'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/> 
      <geometry>
        <box size=".876 .787 .25"/>
      </geometry>
    </collision>

    <visual name='chassis_visual'>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <geometry>
        <box size=".876 .787 .25"/>
      </geometry>
      <material name="orange">
        <color rgba="${255/255} ${108/255} ${10/255} 1.0"/>
      </material>
    </visual>
  </link>
  
  
   <link name='front_left_steering'>
    <inertial>
      <mass value="5.0"/>
      <origin xyz="0 0 0" rpy=" 0 0 0"/>
      <inertia
          ixx="${5*1/12*.1*.1}" ixy="0" ixz="0"
          iyy="${5*1/12*.1*.1}" iyz="0"
          izz="${5*1/12*(.1*.1+.1*.1)}"
      />
    </inertial>

    <collision name='fls_collision'>
